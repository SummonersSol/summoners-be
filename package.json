{
  "name": "summoners-be",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "test": "tsc && cp .env dist/.env && node dist/test.js",
    "start": "node dist/index.js",
    "restart": "tsc && node dist/index.js",
    "migrate": "tsc --skipLibCheck && node dist/migrate.js",
    "migrate:rollback": "tsc && node dist/migrate.js && node dist/rollback.js && node dist/migrate.js",
    "migrate:seed": "tsc --skipLibCheck && node dist/migrate.js && node dist/seed.js",
    "drop": "tsc && node dist/drop.js",
    "drop:seed": "tsc && node dist/drop.js && node dist/migrate.js && node dist/seed.js",
    "seed": "tsc && node dist/seed.js",
    "rollback": "tsc && node dist/rollback.js",
    "rollback:migrate": "tsc && node dist/rollback.js && node dist/migrate.js",
    "rollback:seed": "tsc && node dist/rollback.js && node dist/migrate.js && dist/node seed.js",
    "dev": "nodemon --ext \"ts\" --exec \"tsc && cp .env dist/.env && node dist/index.js\"  --exclude node_modules . || exit 1",
    "dev:windows": "nodemon --ext \"ts\" --exec \"tsc && xcopy \".\\.env\" \".\\dist\\.env\" /Y && node dist/index.js\"  --exclude node_modules . || exit 1",
    "init": "tsc && cp .env dist/.env && node dist/init.js",
    "init:windows": "tsc && xcopy \".\\.env\" \".\\dist\\.env\" /Y && node dist/init.js",
    "tsc": "tsc && cp .env dist/.env",
    "tsc:windows": "tsc && xcopy \".\\.env\" \".\\dist\\.env\" /Y"
  },
  "author": "Kida",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-bubblegum": "^3.1.2",
    "@metaplex-foundation/mpl-candy-machine": "^6.0.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.0",
    "@metaplex-foundation/umi": "^0.9.1",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.1",
    "@orao-network/solana-vrf": "^0.4.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@solana/spl-account-compression": "^0.2.0",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.90.0",
    "app-root-path": "^3.1.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "js-md5": "^0.8.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.7.0",
    "octokit": "^3.1.0",
    "pg": "^8.8.0",
    "prompt-sync": "^4.2.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.7.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.1",
    "@types/js-md5": "^0.7.2",
    "@types/lodash": "^4.14.185",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.8",
    "@types/node-fetch": "^2.6.4",
    "@types/pg": "^8.6.5",
    "@types/prompt-sync": "^4.2.0",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.5.4"
  }
}
